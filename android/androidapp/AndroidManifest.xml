<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="picguess.android"
      android:versionName="1.1" android:versionCode="2">
    <application android:icon="@drawable/picguess_icon" android:label="@string/app_name">
        <activity android:name=".PicGuess"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Authentication"
                  android:label="Authentication">
        </activity>
        <activity android:name=".Signup"
                  android:label="Signup">
        </activity>
        <activity android:name=".Feedback"
                  android:label="Feedback">
        </activity>
        <activity android:name=".Statistics"
                  android:label="Statistics">
        </activity>
        <activity android:name=".Settings"
                  android:label="Settings">
        </activity>
        <service android:name=".DownloadService"
                 android:label="DownloadService">
        </service>

    </application>
    <uses-sdk android:minSdkVersion="4"/>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest> 
